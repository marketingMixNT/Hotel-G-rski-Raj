models:
    CustomScript: { content: text, position: text }
    Slides: { image: text, alt: longtext, sort: 'integer nullable default:NULL' }
    User: { name: string, email: string, email_verified_at: 'timestamp nullable default:NULL', password: string, remember_token: 'string:100 nullable default:NULL' }
    Slide: { image: text, alt: json, sort: 'integer nullable' }
    Advantage: { title: json, description: json, thumbnail: text, sort: 'integer nullable' }
    Offer: { title: json, slug: json, short_desc: json, description: json, thumbnail: text, price: integer, nights: integer, food: string, start_date: dateTime, end_date: dateTime, meta_title: json, meta_desc: json, sort: 'integer nullable' }
    Testimonial: { name: json, source: json, content: json, sort: 'integer nullable' }
    Attraction: { title: json, description: json, images: text, sort: 'integer nullable' }
    Apartment: { name: json, slug: json, surface: integer, person: integer, beds: json, banner_img: text, gallery: text, reservation_link: text, short_desc: json, description: json, sort: 'integer nullable', meta_title: json, meta_desc: json, relationships: { belongsToMany: Amenity } }
    Amenity: { name: json, icons: text, apartment_id: 'unsignedInteger nullable', relationships: { belongsToMany: Apartment } }
    MobileButton: { title: json, icon: text, link: text, sort: 'integer nullable' }
    LocalAttraction: { title: json, description: json, images: text, sort: 'integer nullable' }
